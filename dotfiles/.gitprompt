override_git_prompt_colors() {
  GIT_PROMPT_THEME_NAME="Custom"
  GIT_PROMPT_PREFIX=""
  GIT_PROMPT_SUFFIX=""
  GIT_PROMPT_SEPARATOR="${Black}:"
  GIT_PROMPT_BRANCH="${Green}"
  GIT_PROMPT_SYMBOLS_NO_REMOTE_TRACKING=" "
  GIT_PROMPT_CLEAN="${Cyan}@"
  GIT_PROMPT_STAGED="${Yellow}!"
  GIT_PROMPT_STASHED="${Red}&"
  GIT_PROMPT_CHANGED="${Red}*"
  GIT_PROMPT_CONFLICTS="${Red}?"
  GIT_PROMPT_UNTRACKED="${Yellow}+"
  GIT_PROMPT_SYMBOLS_AHEAD="${Cyan}<"
  GIT_PROMPT_SYMBOLS_BEHIND="${Cyan}>"
  GIT_PROMPT_START_USER="${Blue}\w${ResetColor}"
  GIT_PROMPT_END_USER="${ResetColor} $ "
}

reload_git_prompt_colors "Custom"
